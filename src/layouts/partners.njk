---
layout: base
---
<article class="post h-entry">
    <header>
        <h1 class="p-name">{{ title }}</h1>
    </header>
    
    <div class="content">
        {{ content | safe }}
        <ol class="postlist partnerslist" reversed>
        {% for post in collections.partners | reverse %}
            <li>
                <a href="{{ post.url | url }}">
                    {% if post.data.logo %}
                        <img alt="" src="{{ post.data.logo }}">
                    {% endif %}
                    <div>{{ post.data.title }}</div>
                </a>
            </li>
        {% endfor %}
        </ol>
    </div>

    <footer>
        <a href="/" class="iconlink">
            <svg class="icon" xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" aria-hidden="true">
                <path d="M18 11H8.414l4.293-4.293-1.414-1.414L4.586 12l6.707 6.707 1.414-1.414L8.414 13H18z"/>
            </svg>
            Back to Home
        </a>
    </footer>
</article>